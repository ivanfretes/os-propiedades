<?xml version='1.0' encoding='utf-8'?>
<widget id="com.oscarsantos.com.py" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <allow-intent href="market:*" />
    <icon density="mdpi" src="platforms/android/res/customicon.png" />
    <name>Oscar Santos</name>
    <description>
        Aplicacion de Vendedores, que permite agregar clientes, visualizar disponibilidad de lotes y concretar ventas
    </description>
    <author email="info@oym.com.py" href="http://oym.com.py">
        OyM Systems Group
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
