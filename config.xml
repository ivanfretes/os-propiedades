<?xml version='1.0' encoding='utf-8'?>
<widget id="com.oscarsantos.com.py" version="1.0.12" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Oscar Santos</name>
    <description>
        Aplicacion de Vendedores, que permite agregar clientes, visualizar disponibilidad de lotes y concretar ventas
    </description>
    <author email="info@oym.com.py" href="http://oym.com.py">
        OyM Systems Group
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="platforms/android/res/customicon.png" density="mdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
